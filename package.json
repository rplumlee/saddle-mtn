{
  "name": "gatsby-blog-cosmicjs",
  "description": "Gatsby Blog Cosmic JS",
  "version": "2.0.0",
  "author": "Jazib Sawar <jazibsawar@gmail.com>",
  "bugs": {
    "url": "https://github.com/jazibsawar/gatsby-blog-cosmicjs/issues"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "framer-motion": "^2.9.5",
    "gatsby": "^2.28.2",
    "gatsby-background-image": "^1.3.1",
    "gatsby-image": "^2.7.0",
    "gatsby-plugin-google-analytics": "^2.7.0",
    "gatsby-plugin-offline": "^3.6.0",
    "gatsby-plugin-react-helmet": "^3.6.0",
    "gatsby-plugin-sass": "^2.7.0",
    "gatsby-plugin-sharp": "^2.10.1",
    "gatsby-plugin-typography": "^2.8.0",
    "gatsby-source-cosmicjs": "^1.2.0",
    "gatsby-source-filesystem": "^2.7.0",
    "gatsby-transformer-sharp": "^2.8.0",
    "lodash": "^4.17.20",
    "node-sass": "^4.14.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.1.0",
    "react-intersection-observer": "^8.31.0",
    "react-spring": "^8.0.27",
    "react-typography": "^0.16.19",
    "typeface-lato": "^1.1.13",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.75",
    "typeface-raleway": "^1.1.13",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.21.5",
    "prettier": "^2.2.1"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby-starter-blog#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatsbyjs/gatsby-starter-blog.git"
  },
  "scripts": {
    "dev": "npm run develop",
    "start": "gatsby build; gatsby serve --port $PORT",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "ENABLE_GATSBY_REFRESH_ENDPOINT=true gatsby develop;",
    "poll-content": "while sleep 1; do curl -X POST http://localhost:8000/__refresh; done",
    "build": "gatsby build",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  }
}
